@article{ ISI:000362673800013,
Author = {Singh, Randeep and Qureshi, Qamar and Sankar, Kalyansundram and
   Krausman, Paul R. and Goyal, Surendra Prakash},
Title = {{Estimating occupancy and abundance of caracal in a semi-arid habitat,
   Western India}},
Journal = {{EUROPEAN JOURNAL OF WILDLIFE RESEARCH}},
Year = {{2015}},
Volume = {{61}},
Number = {{6}},
Pages = {{915-918}},
Month = {{DEC}},
Abstract = {{We estimated the occupancy and abundance of caracal (Felis caracal)
   using camera traps in the core area of Ranthambhore Tiger Reserve (RTR),
   a semi-arid habitat, in Western India, from 2006 to 2009. Caracals were
   detected in 17 of 28 grids, yielding a na < ve occupancy estimate of
   0.61. After accounting for imperfect detection (P = 0.17 +/- 0.04
   {[}SE]), the proportion of sites occupied (occupancy) was 87 \%. Caracal
   abundance was similar to 28.17 +/- 3.02 individuals in the study area.
   Based on the total area available for caracals (588 km(2)), the average
   expected density was as 4.8 caracals per 100 km(2). Information on
   caracal occupancy and abundance in the Indian sub-continent has not been
   documented or published, and this is the first attempt to estimate
   occupancy rates and the approximate population size of the caracal in
   RTR. Hence, our data should be used as baseline information for making
   future management and conservation strategies for the species in this
   and similar landscapes.}},
