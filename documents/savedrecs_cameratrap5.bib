@article{ ISI:000246103000025,
Author = {Trolle, Mogens and Bissaro, Marcos Cesar and Prado, Helbert Medeiros},
Title = {{Mammal survey at a ranch of the Brazilian Cerrado}},
Journal = {{BIODIVERSITY AND CONSERVATION}},
Year = {{2007}},
Volume = {{16}},
Number = {{4}},
Pages = {{1205-1211}},
Month = {{APR}},
Abstract = {{The wildlife of the Brazilian Cerrado is threatened by large-scale
   habitat loss, in particular due to conversion to agricultural land. It
   is essential to study how the mammal fauna copes with the highly
   fragmented, human-influenced, non-protected landscape. The paper
   presents the results of a survey of the large to medium-sized mammals of
   a typical cattle ranch with a mixture of human-created and natural
   vegetation types. We recorded 18 species. Surprisingly, several species
   were found to still thrive in the area, however, many species are rare
   or have become extinct. We conclude the paper with comments relevant for
   the conservation of mammals in the Cerrado as a whole.}},
