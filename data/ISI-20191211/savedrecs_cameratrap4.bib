@article{ ISI:000355857300003,
Author = {Le Saout, Soizic and Martin, Jean-Louis and Blanchard, Pierrick and
   Cebe, Nicolas and Mark Hewison, A. J. and Rames, Jean-Luc and
   Chamaille-Jammes, Simon},
Title = {{Seeing a Ghost? Vigilance and Its Drivers in a Predator-free World}},
Journal = {{ETHOLOGY}},
