@article{ ISI:000466104200034,
Author = {Jimenez, Jose and Chandler, Richard and Tobajas, Jorge and Descalzo,
   Esther and Mateo, Rafael and Ferreras, Pablo},
Title = {{Generalized spatial mark-resight models with incomplete identification:
   An application to red fox density estimates}},
Journal = {{ECOLOGY AND EVOLUTION}},
