@article{ ISI:000350517300009,
Author = {Pryde, Elizabeth C. and Holland, Greg J. and Watson, Simon J. and
   Turton, Stephen M. and Nimmo, Dale G.},
Title = {{Conservation of tropical forest tree species in a native timber
   plantation landscape}},
Journal = {{FOREST ECOLOGY AND MANAGEMENT}},
