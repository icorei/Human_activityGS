@article{ ISI:A1996UJ67900058,
Author = {Gledhill, CT and LyczkowskiShultz, J and Rademacher, K and Kargard, E
   and Crist, G and Grace, MA},
Title = {{Evaluation of video and acoustic index methods for assessing reef-fish
   populations}},
Journal = {{ICES JOURNAL OF MARINE SCIENCE}},
