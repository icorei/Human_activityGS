Title = {{RANGE EXTENSION OF THE NORTHERN NAKED-TAILED ARMADILLO (CABASSOUS
   CENTRALIS) IN SOUTHERN MEXICO}},
Journal = {{WESTERN NORTH AMERICAN NATURALIST}},
Year = {{2017}},
Volume = {{77}},
