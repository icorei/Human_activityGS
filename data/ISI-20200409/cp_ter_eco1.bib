Title = {{Application of species distribution models in stream ecosystems: the
   challenges of spatial and temporal scale, environmental predictors and
   species occurrence data}},
Journal = {{FUNDAMENTAL AND APPLIED LIMNOLOGY}},
Year = {{2015}},
Volume = {{186}},
Number = {{1-2, SI}},
