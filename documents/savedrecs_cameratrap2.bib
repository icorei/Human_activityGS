@article{ ISI:000425494400010,
Author = {Kittle, Andrew M. and Watson, Anjali C.},
Title = {{Density of leopards (Panthera pardus kotiya) in Horton Plains National
   Park in the Central Highlands of Sri Lanka}},
Journal = {{MAMMALIA}},
Year = {{2018}},
Volume = {{82}},
Number = {{2}},
Pages = {{183-187}},
Month = {{MAR}},
Abstract = {{Quantitative ecological data needs to inform management of the
   endangered, endemic Sri Lankan leopard. Estimating habitat-specific
   leopard density and prey availability provides important baselines and
   improved understanding of the island-wide population. We used remote
