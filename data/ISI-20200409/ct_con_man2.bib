@article{ ISI:000271732400026,
Author = {Johnson, Arlyne and Vongkhamheng, Chanthavy and Saithongdam, Thavisouk},
Title = {{The diversity, status and conservation of small carnivores in a montane
   tropical forest in northern Laos}},
