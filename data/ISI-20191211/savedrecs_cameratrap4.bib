@article{ ISI:000352095300024,
Author = {Lerone, Laura and Carpaneto, Giuseppe M. and Loy, Anna},
Title = {{Why Camera Traps Fail to Detect a Semi-Aquatic Mammal: Activation
   Devices as Possible Cause}},
Journal = {{WILDLIFE SOCIETY BULLETIN}},
Year = {{2015}},
Volume = {{39}},
Number = {{1}},
Pages = {{193-196}},
Month = {{MAR}},
Abstract = {{Camera-trapping represents a powerful tool in wildlife research,
   particularly when dealing with elusive and rare species such as otters
   (Lutra spp.). Nevertheless, detection problems arise when trying to
   detect otters with camera traps. We hypothesized that the temperature
   difference between the otter emerging from the water and the environment
   was too low to be detected by the standard passive infrared sensors
   (PIR). During June-September of 2010 and 2011 in the Sangro River basin
   in central Italy, we designed and tested a new pressure trigger and
   compared its effectiveness with that of the standard PIR. Results are
   encouraging and suggest that the new trigger could be able to detect
   otters and other semi-aquatic mammals. (c) 2015 The Wildlife Society.}},
