@article{ ISI:000367807500006,
Author = {Royle, J. Andrew and Fuller, Angela K. and Sutherland, Chris},
Title = {{Spatial capture-recapture models allowing Markovian transience or
   dispersal}},
Journal = {{POPULATION ECOLOGY}},
