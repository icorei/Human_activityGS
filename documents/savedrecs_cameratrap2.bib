@article{ ISI:000405190600007,
Author = {Rich, Lindsey N. and Davis, Courtney L. and Farris, Zach J. and Miller,
   David A. W. and Tucker, Jody M. and Hamel, Sandra and Farhadinia,
   Mohammad S. and Steenweg, Robin and Di Bitetti, Mario S. and Thapa,
