@article{ ISI:000208368900010,
Author = {Skerl, Kevin L.},
Title = {{Spiders in conservation planning: a survey of US natural heritage
   programs}},
Journal = {{JOURNAL OF INSECT CONSERVATION}},
Year = {{1999}},
Volume = {{3}},
Number = {{4}},
Pages = {{341-347}},
Month = {{DEC}},
Abstract = {{Although spiders play important roles in terrestrial ecosystems and are
   negatively impacted by human activity, they have received little
   attention from the US conservation community. Information gaps may
   prohibit the inclusion of spiders in conservation planning. Conservation
   priority setting, environmental review and local land-use planning
   activities in the US increasingly rely on information compiled by the
   Natural Heritage Network. A review of heritage database content and a
   survey of natural heritage programs both indicate a paucity of compiled
   data on the US spider fauna. Heritage programs are probably hampered by
   the unavailability of current species checklists, identification
   services and keys, and a general unfamiliarity with the fauna. Clearly,
   further development of information on rare or imperiled spiders will
   depend on increased communication and collaboration with arachnologists.}},
