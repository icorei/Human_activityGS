@article{ ISI:000475563600004,
Author = {Zhang, Yuke and Liu, Xuehua and Lv, Zixin and Zhao, Xiangyu and Yang,
   Xingzhong and Jia, Xiaodong and Sun, Wanlong and He, Xiangbo and He,
   Baishuo and Cai, Qiong and Zhu, Yun},
Title = {{Animal diversity responding to different forest restoration schemes in
   the Qinling Mountains, China}},
Journal = {{ECOLOGICAL ENGINEERING}},
Year = {{2019}},
Volume = {{136}},
Pages = {{23-29}},
Month = {{OCT}},
Abstract = {{China's primary forest has gone through the extensive deforestation and
   further restoration since 1950s. However, little is known about how
   animal diversity responds to restoration. Here, we used camera trapping
   to monitor diversity of large-and-medium mammals for three years
   (2014-2017) in four forest habitat types in the Qinling Mountains:
   high-elevation primary forest, low-elevation primary forest, secondary
   forest, and plantation forest. The species richness did not vary between
   forest types (n = 17-18). But the relative abundant index (RAI) was
   higher in low-elevation primary (3.00\%), followed by plantation forest
   (1.77\%), secondary forest (0.89\%), and high-elevation primary forest
   (0.74\%), respectively. In term of habitat using, low-elevation primary
   forest was the most preferred habitat by the animals (11 species),
   followed by high-elevation forest (5 species), and plantation forest (4
   species), while secondary forest was only preferred by 1 species.
   Species' preferences on forest habitat types were stable during daytime
   and night-time but significantly changed in different seasons. Our study
   indicated that both plantation forest and secondary forest in the
   Qinling Mountains played important roles of supporting animal species.
   Thus a comprehensive management is required for biodiversity
   conservation in the Qinling Mountains.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Liu, XH (Reprint Author), Tsinghua Univ, State Key Joint Lab Environm Simulat \& Pollut Con, Beijing 100084, Peoples R China.
