@article{ ISI:000317044300001,
Author = {Sollmann, Rahel and Gardner, Beth and Parsons, Arielle W. and Stocking,
   Jessica J. and McClintock, Brett T. and Simons, Theodore R. and Pollock,
   Kenneth H. and O'Connell, Allan F.},
Title = {{A spatial mark-resight model augmented with telemetry data}},
