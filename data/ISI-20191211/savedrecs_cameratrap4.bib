@inproceedings{ ISI:000368235000013,
Author = {Meek, Paul D. and Pittet, Andre},
Editor = {{Ballard, G and Banks, P and Claridge, A and Sanderson, J and Swann, D}},
Title = {{A review of the ultimate camera trap for wildlife research and
   monitoring}},
Booktitle = {{CAMERA TRAPPING: WILDLIFE MANAGEMENT AND RESEARCH}},
Year = {{2014}},
Pages = {{101-109}},
Note = {{1st International Colloquium on Camera Trapping in Wildlife Management
   and Research, Sydney, AUSTRALIA, SEP 13-14, 2012}},
Abstract = {{The use of camera traps in science and management throughout the world
   is eclectic. Despite its rapid uptake as a tool, there is still a
   considerable amount to learn about the use of camera traps in wildlife
   research. There is not widespread acceptance and satisfaction with
   camera traps in the scientific fraternity and the search for solutions
   is being pursued. The problems encountered include accurate detection of
   targets, quality of images, flash type, weather proofing, robustness of
   the device, varying speeds between models, and generally any feature
   that limits the gathering of robust datasets. The breadth of knowledge
   of camera trap practitioners provides a useful platform upon which to
   design the perfect camera trap. Here we discuss the findings of Meek and
   Pittet ( 2012) and consider how the recommendations of the ultimate
   camera trap device can contribute to resolving some of the challenges of
