Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Noss, A (Reprint Author), Wildlife Conservat Soc, Casilla 6272, Santa Cruz, Bolivia.
   Wildlife Conservat Soc, Santa Cruz, Bolivia.}},
DOI = {{10.1017/S0952836903004655}},
ISSN = {{0952-8369}},
EISSN = {{1469-7998}},
Keywords = {{survey; camera trap; landscape species; Panthera onca; Chaco}},
Keywords-Plus = {{CAPTURE-RECAPTURE MODELS; PHOTOGRAPHIC RATES; ESTIMATE DENSITIES;
