@article{ ISI:000506007600001,
Author = {Paddock, Christina Lynette and Bruford, Michael William and McCabe,
   Grainne Michelle},
Title = {{Estimating the population size of the Sanje mangabey (Cercocebus sanjei)
   using acoustic distance sampling}},
Journal = {{AMERICAN JOURNAL OF PRIMATOLOGY}},
