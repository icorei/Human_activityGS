@article{ ISI:000259544200015,
Author = {Edgar, Graham J. and Langhammer, Penny F. and Allen, Gerry and Brooks,
   Thomas M. and Brodie, Juliet and Crosse, William and De Silva, Naamal
   and Fishpool, Lincoln D. C. and Foster, Matthew N. and Knox, David H.
   and Mccosker, John E. and McManus, Roger and Millar, Alan J. K. and
   Mugo, Robinson},
Title = {{Key biodiversity areas as globally significant target sites for the
   conservation of marine biological diversity}},
Journal = {{AQUATIC CONSERVATION-MARINE AND FRESHWATER ECOSYSTEMS}},
Year = {{2008}},
Volume = {{18}},
Number = {{6}},
Pages = {{969-983}},
Month = {{SEP-OCT}},
Abstract = {{1. Recent approaches to the planning of marine protected area (MPA)
   networks for biodiversity conservation often stress the need for a
   representative coverage of habitat types while aiming to minimize
   impacts on resource users. As typified by planning for the Australian
   South-east Marine Region, this strategy can be manipulated by political
   processes, with consequent biased siting of MPAs. Networks thus created
   frequently possess relatively low value for biodiversity conservation,
   despite significant costs in establishment and maintenance.
   2. Such biases can be minimized through application of the data-driven
   and species-based concept of key biodiversity areas (KBAs).
   3. By mapping locations of threatened species and populations that are
   highly aggregated in time or space, the KBA process allows marine sites
   of global biodiversity significance to be systematically identified as
   priority conservation targets. Here. the value of KBAs for marine
   conservation planning is outlined. and guidelines and provisional
   criteria for their application provided. Copyright (C) 2008 John Wiley
   \& Sons. Ltd.}},
