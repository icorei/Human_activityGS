@article{ ISI:000403577800012,
Author = {Richardson, Matthew L.},
Title = {{DAILY AND MONTHLY ACTIVITY OF BROWN BEARS (URSUS ARCTOS) NEAR A PROPOSED
   INDUSTRIAL PROJECT IN COASTAL BRITISH COLUMBIA}},
