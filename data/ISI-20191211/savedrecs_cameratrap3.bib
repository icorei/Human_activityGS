Author = {Austin, Caitlin and Tuft, Katherine and Ramp, Daniel and Cremona, Teigan
   and Webb, Jonathan K.},
Title = {{Bait preference for remote camera trap studies of the endangered
   northern quoll (Dasyurus hallucatus)}},
Journal = {{AUSTRALIAN MAMMALOGY}},
