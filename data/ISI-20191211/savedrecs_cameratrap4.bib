@inproceedings{ ISI:000368235000023,
Author = {Taylor, Brendan and Goldingay, Ross},
Editor = {{Ballard, G and Banks, P and Claridge, A and Sanderson, J and Swann, D}},
Title = {{Using camera traps to monitor use of roadside glide poles and rope
   canopy-bridges by Australian gliding mammals}},
Booktitle = {{CAMERA TRAPPING: WILDLIFE MANAGEMENT AND RESEARCH}},
Year = {{2014}},
Pages = {{245-252}},
Note = {{1st International Colloquium on Camera Trapping in Wildlife Management
   and Research, Sydney, AUSTRALIA, SEP 13-14, 2012}},
Abstract = {{Glide poles and rope canopy-bridges for arboreal mammals are becoming
   features of major road upgrades throughout eastern Australia. While
   these structures are designed to reduce the barrier effect of roads and
   reconnect habitat, little is known about their use by gliding mammals.
   This paper describes the implementation of camera traps to monitor
   glider use of three different road crossing structures: roadside glide
   poles; glide poles on a land-bridge; and the combination of glide poles
   and rope on a land-bridge. Each structure was monitored for 310, 150 and
   536 nights, respectively. Camera traps revealed frequent use of each
   structure type by squirrel gliders (Petaurus norfolcensis). Paired
   cameras proved effective in demonstrating that complete road crossings
   had occurred. We discuss these results with reference to some of the
   technical challenges associated with adapting trail camera technology to
   such a monitoring context and how camera technology has dramatically
   expanded our understanding of crossing structure use by arboreal
   mammals.}},
