@article{ ISI:000269484100022,
Author = {Sarmento, Pedro and Cruz, Joana and Eira, Catarina and Fonseca, Carlos},
Title = {{Evaluation of Camera Trapping for Estimating Red Fox Abundance}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
Year = {{2009}},
Volume = {{73}},
Number = {{7}},
Pages = {{1207-1212}},
Month = {{SEP}},
Abstract = {{The nature reserve Serra da Malcata, Portugal, was recently considered a
   site for Iberian lynx (Lynx pardinus) reintroduction. Because of
   potential disease risk posed by red foxes (Vulpes vulpes) in the area, a
   reliable estimate of fox abundance was critical for a dependable
   reintroduction program. We adapted camera-trapping techniques for
   estimating red fox abundance in the reserve. From July 2005 to August
   2007, we conducted 7 camera-trapping sessions, allowing for individual
   identification of foxes by physical characteristics. We estimated
   abundance using the heterogeneity (Mh) model of the software program
   CAPTURE. Estimated density ranged from 0.91 +/- 0.12 foxes/km(2) to 0.74
   +/- 0.02 foxes/km(2). By estimating red fox density, it is possible to
   define the number of foxes that must be sampled to assess the presence
   of potential fox-transmitted diseases that may affect lynx
   reintroduction. (JOURNAL OF WILDLIFE MANAGEMENT 73(7): 1207-1212; 2009)}},
