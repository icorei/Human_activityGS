@article{ ISI:000233507300011,
Author = {Rizzo, W},
Title = {{Return of the jaguar?}},
Journal = {{SMITHSONIAN}},
Year = {{2005}},
Volume = {{36}},
Number = {{9}},
Pages = {{20+}},
Month = {{DEC}},
Abstract = {{Camera-traps set up in Amado, Arizona have captured the third sighting
   of a jaguar in recent times and if the cat is female, it will be the
   first one known in the USA in 40 years. In 1969 Arizona outlawed most
   jaguar hunting but with no females there was little hope of the
   population surviving. The jaguars may be living and breeding again in
   Arizona but experts from the Wildlife Conservation Society believe they
   may have strayed from their territory in Sonora, Mexico on the hunt for
   food.}},
