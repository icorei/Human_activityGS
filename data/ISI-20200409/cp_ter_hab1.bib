@article{ ISI:000265440700016,
Author = {Thorne, James H. and Girvetz, Evan H. and McCoy, Michael C.},
Title = {{Evaluating Aggregate Terrestrial Impacts of Road Construction Projects
   for Advanced Regional Mitigation}},
Journal = {{ENVIRONMENTAL MANAGEMENT}},
