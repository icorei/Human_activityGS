@article{ ISI:000486256300002,
Author = {Karssene, Yamna and Godinho, Raquel and Chammem, Mohsen and Cocchiararo,
   Berardino and Nouira, Said and Nowak, Carsten},
Title = {{Noninvasive DNA sampling and camera trapping suggest dramatic regional
   conservation status of an understudied carnivore: the Ruppell's fox in
   Tunisia}},
Journal = {{JOURNAL OF NATURAL HISTORY}},
Year = {{2019}},
Volume = {{53}},
Number = {{23-24}},
Pages = {{1439-1449}},
Month = {{JUN 26}},
Abstract = {{Ruppell's fox, Vulpes rueppellii, is one of the four fox species
   occurring in North Africa. Its population status and ecology remains
   largely unknown. Here, we collected occurrence data from camera trapping
   and scat-collection based mitochondrial control region sequencing to
   assess the distribution range and status of the species across potential
   habitats in Tunisia. The collection and analysis of 342 scat samples
   resulted in 234 sequences of the mitochondrial control region. Our
   results did not reveal any evidence of Ruppell's fox presence in
   Tunisia. In contrast, three other mesocarnivores were commonly recorded
   by camera trapping and molecular data, namely African wolf `Canis
   lupaster', red fox `Vulpes vulpes' and fennec fox `V. zerda'. The three
   species exhibited distinct geographical affinities, which are reflected
   in different habitat selection patterns. To the best of our knowledge,
   this is the first study focusing on the distribution of V. rueppellii in
   North Africa using both camera trapping and noninvasive genetic methods.
   In terms of conservation, it seems urgent to characterise the status of
   this species in North Africa, and particularly in Tunisia.}},
Publisher = {{TAYLOR \& FRANCIS LTD}},
Address = {{2-4 PARK SQUARE, MILTON PARK, ABINGDON OR14 4RN, OXON, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Karssene, Y (Reprint Author), Univ Tunis El Manar, Dept Biol, Fac Sci Tunis, Tunis, Tunisia.
