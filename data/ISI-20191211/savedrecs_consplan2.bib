@article{ ISI:000250644100009,
Author = {Chown, Steven L. and Convey, Peter},
Title = {{Spatial and temporal variability across life's hierarchies in the
   terrestrial Antarctic}},
Journal = {{PHILOSOPHICAL TRANSACTIONS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES}},
