@article{ ISI:000400078900001,
Author = {Apps, Peter and Claase, Megan and Yexley, Ben and McNutt, J. Weldon},
Title = {{Interspecific responses of wild African carnivores to odour of
   3-mercapto-3-methylbutanol, a component of wildcat and leopard urine}},
