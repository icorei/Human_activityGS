Title = {{Biodiversity Inventory and Conservation Opportunity of Suwi Wetlands,
   Muara Ancalong, East Kalimantan, Indonesia}},
Booktitle = {{1ST INTERNATIONAL CONFERENCE ON MATHEMATICS, SCIENCE AND COMPUTER
   SCIENCE (ICMSC) 2016: SUSTAINABILITY AND ECO GREEN INNOVATION IN
   TROPICAL STUDIES FOR GLOBAL FUTURE}},
Series = {{AIP Conference Proceedings}},
