@article{ ISI:000219041000010,
Author = {Kalbi, Siavash and Fallah, Asghar and Hojjati, Seyed Mohammad},
Title = {{Using and comparing two nonparametric methods (CART and RF) and SPOT-HRG
   satellite data to predictive tree diversity distribution}},
Journal = {{NUSANTARA BIOSCIENCE}},
