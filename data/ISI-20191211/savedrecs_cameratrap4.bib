@inproceedings{ ISI:000368235000021,
Author = {Welbourne, Dustin},
Editor = {{Ballard, G and Banks, P and Claridge, A and Sanderson, J and Swann, D}},
Title = {{Using camera traps to survey diurnal terrestrial reptiles: a proof of
   concept}},
Booktitle = {{CAMERA TRAPPING: WILDLIFE MANAGEMENT AND RESEARCH}},
Year = {{2014}},
Pages = {{225-232}},
Note = {{1st International Colloquium on Camera Trapping in Wildlife Management
   and Research, Sydney, AUSTRALIA, SEP 13-14, 2012}},
Abstract = {{The use of automatically triggered cameras in fauna research, especially
   those triggered by passive infrared sensors, has grown rapidly in the
   recent decades. Little research has taken place exploring the use of
   automatically triggered cameras for reptiles, principally due to
   limitations, both real and perceived, with the technology. However, by
   altering the method in which these systems are deployed, passive
   infrared automatically triggered cameras can be used for detecting
   diurnal terrestrial reptiles. The Camera Overhead Augmented Temperature
   protocol establishes a camera above an alternative substrate that
   augments the temperature in the detection zone. By augmenting the
   temperature, animals that have a similar temperature to the surrounding
   environment, for example reptiles, can be detected. This paper expands
   on Welbourne (2013), describing the background experiments to that work
   and details a method for using passive infrared automatically triggered
   cameras for surveying reptiles.}},
