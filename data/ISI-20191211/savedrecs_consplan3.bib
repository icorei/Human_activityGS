@article{ ISI:A1992HC10800001,
Author = {TRILLMICH, F},
Title = {{CONSERVATION PROBLEMS ON GALAPAGOS - THE SHOWCASE OF EVOLUTION IN DANGER}},
Journal = {{NATURWISSENSCHAFTEN}},
