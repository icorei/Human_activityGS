@article{ ISI:000298586200020,
Author = {Edward Gray, Thomas Neill and Prum, Sovanna},
Title = {{Leopard density in post-conflict landscape, Cambodia: Evidence from
   spatially explicit capture-recapture}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
