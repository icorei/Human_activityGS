@article{ ISI:000248967400014,
Author = {Reyers, B. and Rouget, M. and Jonas, Z. and Cowling, R. M. and Driver,
   A. and Maze, K. and Desmet, P.},
Title = {{Developing products for conservation decision-making: lessons from a
   spatial biodiversity assessment for South Africa}},
