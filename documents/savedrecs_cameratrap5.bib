Author = {Sathyakumar, Sambandam and Bashir, Tawqir and Bhattacharya, Tapajit and
   Poudyal, Kamal},
Title = {{Assessing mammal distribution and abundance in intricate eastern
   Himalayan habitats of Khangchendzonga, Sikkim, India}},
Journal = {{MAMMALIA}},
