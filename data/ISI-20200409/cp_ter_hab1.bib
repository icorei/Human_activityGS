@article{ ISI:000327437900008,
Author = {Caldwell, Iain R. and Gergel, Sarah E.},
Title = {{Thresholds in seascape connectivity: influence of mobility, habitat
   distribution, and current strength on fish movement}},
Journal = {{LANDSCAPE ECOLOGY}},
