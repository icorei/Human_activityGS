@article{ ISI:000422760500020,
Author = {Srbek-Araujo, Ana Carolina and Da Cunha, Cristina Jaques and Roper,
   James Joseph},
Title = {{Post-dispersal seed predation by Atlantic Forest squirrels monitoring
   lowland tapir latrines}},
Journal = {{TROPICAL ECOLOGY}},
