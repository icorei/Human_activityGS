@article{ ISI:000401486400006,
Author = {Colley, Michael and Lougheed, Stephen C. and Otterbein, Kenton and
   Litzgus, Jacqueline D.},
Title = {{Mitigation reduces road mortality of a threatened rattlesnake}},
Journal = {{WILDLIFE RESEARCH}},
