@article{ ISI:000377561700074,
Author = {Stommel, Claudia and Hofer, Heribert and East, Marion L.},
Title = {{The Effect of Reduced Water Availability in the Great Ruaha River on the
   Vulnerable Common Hippopotamus in the Ruaha National Park, Tanzania}},
Journal = {{PLOS ONE}},
Year = {{2016}},
Volume = {{11}},
Number = {{6}},
Month = {{JUN 8}},
Abstract = {{In semi-arid environments, `permanent' rivers are essential sources of
   surface water for wildlife during `dry' seasons when rainfall is limited
   or absent, particularly for species whose resilience to water scarcity
   is low. The hippopotamus (Hippopotamus amphibius) requires submersion in
   water to aid thermoregulation and prevent skin damage by solar
   radiation; the largest threat to its viability are human alterations of
   aquatic habitats. In the Ruaha National Park (NP), Tanzania, the Great
   Ruaha River (GRR) is the main source of surface water for wildlife
   during the dry season. Recent, large-scale water extraction from the GRR
   by people upstream of Ruaha NP is thought to be responsible for a
   profound decrease in dry season water-flow and the absence of surface
   water along large sections of the river inside the NP. We investigated
   the impact of decreased water flow on daytime hippo distribution using
   regular censuses at monitoring locations, transects and camera trap
   records along a 104km section of the GRR within the Ruaha NP during two
   dry seasons. The minimum number of hippos per monitoring location
   increased with the expanse of surface water as the dry seasons
   progressed, and was not affected by water quality. Hippo distribution
   significantly changed throughout the dry season, leading to the
   accumulation of large numbers in very few locations. If surface water
   loss from the GRR continues to increase in future years, this will have
   serious implications for the hippo population and other water dependent
   species in Ruaha NP.}},
