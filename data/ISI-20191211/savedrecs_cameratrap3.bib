@article{ ISI:000373647400003,
Author = {Guerra, Ana Sofia and Micheli, Fiorenza and Wood, Chelsea L.},
Title = {{Ecology of a Vulnerable Shorebird across a Gradient of Habitat
   Alteration: Bristle-Thighed Curlews (Numenius tahitiensis) (Ayes:
   Charadriiformes) on Palmyra Atoll}},
Journal = {{PACIFIC SCIENCE}},
Year = {{2016}},
Volume = {{70}},
Number = {{2}},
Pages = {{159-174}},
Month = {{APR}},
Abstract = {{Palmyra Atoll, USA, in the Central Pacific, has remained mostly
   uninhabited since construction and abandonment of a U.S. naval base
   during World War II. However, the effects of Navy modifications have
   persisted, affecting physical conditions and benthic habitat quality of
   Palmyra's lagoon sand flats. Sand flats provide important nonbreeding
   habitat for Bristle-thighed Curlews (Numenius tahitiensis), a migratory
   shorebird listed as ``vulnerable{''} by the International Union for
   Conservation of Nature and Natural Resources. We used camera trapping,
   observations of focal individuals, and quantification of prey
   availability and sediment characteristics to assess curlew habitat use
   across different levels of historical anthropogenic impact. Habitat
   preferences were not determined by the degree of land reclamation impact
   on lagoon flat habitat, although the two most preferred sites were both
   highly impacted. Curlew abundance was most strongly correlated with
   availability of prey items, such as spionid polychaetes (Malacoceros
   sp.). Our findings provide new ecological information on a shorebird
   species that is rarely studied and provides useful information for
   habitat management at Palmyra Atoll and other curlew wintering grounds.}},
