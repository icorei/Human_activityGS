@article{ ISI:000269692700001,
Author = {Thorn, Michelle and Scott, Dawn M. and Green, Matthew and Bateman,
   Philip W. and Cameron, Elissa Z.},
Title = {{Estimating brown hyaena occupancy using baited camera traps}},
