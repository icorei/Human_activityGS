@article{ ISI:000369817500005,
Author = {Logan, Mary Kotschwar},
Title = {{Assessing site occupancy of Mohave ground squirrels: Implications for
   conservation}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
