@article{ ISI:000274243800018,
Author = {Gardner, Beth and Royle, J. Andrew and Wegan, Michael T. and Rainbolt,
   Raymond E. and Curtis, Paul D.},
Title = {{Estimating Black Bear Density Using DNA Data From Hair Snares}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
