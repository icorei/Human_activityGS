@inproceedings{ ISI:000368235000017,
Author = {Hradsky, Bronwyn A.},
Editor = {{Ballard, G and Banks, P and Claridge, A and Sanderson, J and Swann, D}},
Title = {{More than just presence-absence: camera traps reveal fine scale resource
   partitioning by the ubiquitous swamp wallaby (Wallabia bicolor)}},
Booktitle = {{CAMERA TRAPPING: WILDLIFE MANAGEMENT AND RESEARCH}},
Year = {{2014}},
Pages = {{181-188}},
Note = {{1st International Colloquium on Camera Trapping in Wildlife Management
   and Research, Sydney, AUSTRALIA, SEP 13-14, 2012}},
Abstract = {{Presence-absence studies are often used to investigate relationships
   between species and their environment. However, presence-absence
   analyses cannot be conducted if a species is `too common' and occurs at
   every site. Motion cameras are an effective tool for determining
   species' occurrence, but the detailed data they collect on the date and
   time of detections can also help tease out more complex temporal
   relationships. This study used motion cameras to investigate the impacts
   of shrub encroachment on a native mammal, the swamp wallaby (Wallabia
   bicolor). The study was conducted in an Australian forest reserve which
   has been heavily encroached by the indigenous shrub yarra burgan (Kunzea
   leptospermoides). W. bicolor was detected at every site, regardless of
   the level of shrub encroachment or other habitat variables. However,
   there was a strong interaction between shrub encroachment and the time
   of day at which wallabies were detected. Although wallabies were present
   at every site, they used shrub-encroached sites for shelter during the
   day and foraged in open areas at night. The ability of motion cameras to
   collect data continuously and in all conditions provides us with a
   powerful tool to further our understanding of the behaviour of both
   cryptic and ubiquitous species.}},
