@article{ ISI:000472660900008,
Author = {Granados, Alys and Bernard, Henry and Brodie, Jedediah F.},
Title = {{The influence of logging on vertebrate responses to mast fruiting}},
Journal = {{JOURNAL OF ANIMAL ECOLOGY}},
