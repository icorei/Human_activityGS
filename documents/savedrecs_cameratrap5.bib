@article{ ISI:000253311700007,
Author = {Bagla, Pallava},
Title = {{Species conservation - Tigers in decline, Indian survey finds}},
Journal = {{SCIENCE}},
Year = {{2008}},
Volume = {{319}},
Number = {{5866}},
Pages = {{1027}},
Month = {{FEB 22}},
Abstract = {{The use of new sampling techniques has cut by half the estimated number
   of wild tigers in India. Experts say the decline reflects more than just
   a change in methodology, poaching, human encroachment, and habitat loss
   take a heavy toll. Off icials at the Wildlife Institute of India in
   Dehradun, which conducted the new study, adopted the new approach that
   used camera-trap surveys, line-transect sampling, and occupancy
   modeling. However, it omitted a vast marshy area known as the Sundarbans
   forest for which researchers had not developed a sampling method.}},
