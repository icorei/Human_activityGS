@inproceedings{ ISI:000451372400042,
Author = {Epifani, Rosa and Amato, Federico and Murgante, Beniamino and Nole,
   Gabriele},
Editor = {{Gervasi, O and Murgante, B and Misra, S and Borruso, G and Torre, CM and Rocha, AMAC and Taniar, D and Apduhan, BO and Stankova, E and Cuzzocrea, A}},
