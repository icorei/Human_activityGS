@inproceedings{ ISI:000368235000033,
Author = {Meek, Paul D. and Butler, Des},
Editor = {{Ballard, G and Banks, P and Claridge, A and Sanderson, J and Swann, D}},
Title = {{Now we can `see the forest and the trees, too', but there are risks:
   camera trapping and privacy law in Australia}},
Booktitle = {{CAMERA TRAPPING: WILDLIFE MANAGEMENT AND RESEARCH}},
Year = {{2014}},
Pages = {{331-345}},
Note = {{1st International Colloquium on Camera Trapping in Wildlife Management
   and Research, Sydney, AUSTRALIA, SEP 13-14, 2012}},
Abstract = {{The use of camera traps in wildlife management is an increasingly common
   practice. A phenomenon that is also becoming more common is for such
   camera traps to unintentionally film individuals engaged in a variety of
   activities, ranging from the innocent to the nefarious and including
   lewd or potentially embarrassing behaviour. It is therefore possible for
   the use of camera traps to accidentally encroach upon the privacy rights
   of people who venture into the area of surveillance. In this paper we
   describe the legal framework of privacy in Australia and discuss the
   potential risk of this `sleeping tiger' for users of camera traps. We
   also present the results of a survey of camera trap users to assess the
   frequency of such unintended captures and the nature of activity being
   filmed before discussing the practical implications of these laws for
   camera traps users in this country and make recommendations.}},
