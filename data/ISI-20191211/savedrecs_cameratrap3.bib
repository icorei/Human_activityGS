Journal = {{DATA IN BRIEF}},
Year = {{2016}},
Volume = {{6}},
Pages = {{783-792}},
Month = {{MAR}},
Abstract = {{These data provide additional information relevant to the frequency of
   fisher detections by camera traps, and single-season occupancy and local
   persistence of fishers in small patches of forest habitats detailed
