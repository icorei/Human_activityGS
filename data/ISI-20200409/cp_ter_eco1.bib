@article{ ISI:000252895700014,
Author = {Tallis, Heather and Ferdana, Zach and Gray, Elizabeth},
Title = {{Linking terrestrial and marine conservation planning and threats
   analysis}},
Journal = {{CONSERVATION BIOLOGY}},
