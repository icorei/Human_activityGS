@article{ ISI:000498874300009,
Author = {Farashi, Azita and Alizadeh-Noughani, Mohammad},
Title = {{Conservation of Pleske's racerunner (Eremias pleskei) in a changing
   climate}},
Journal = {{ANNALES ZOOLOGICI FENNICI}},
