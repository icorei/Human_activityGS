@inproceedings{ ISI:000368235000008,
Author = {Pittet, Malini and Bennett, Peter},
Editor = {{Ballard, G and Banks, P and Claridge, A and Sanderson, J and Swann, D}},
Title = {{Examining the state of biodiversity using camera traps in the Pacaya
   Samiria National Reserve, Peru}},
Booktitle = {{CAMERA TRAPPING: WILDLIFE MANAGEMENT AND RESEARCH}},
Year = {{2014}},
Pages = {{53-60}},
Note = {{1st International Colloquium on Camera Trapping in Wildlife Management
   and Research, Sydney, AUSTRALIA, SEP 13-14, 2012}},
Abstract = {{Assessment of the distribution, abundance, density and dynamics of
   animal populations in challenging landscapes is problematic. We aimed to
   develop population monitoring methods that were feasible in the Pacaya
   Samiria National Reserve, a jungle area characterised by periodically
   flooded forest. Camera traps and line transects were used to sample the
   study area in 2009 and 2011. The camera trapping effort of 1927 trap
   days yielded a total of 7858 photographs of which 4683 were used in the
   analysis. A total of 30 terrestrial mammal species as well as eight game
   bird species were captured on the camera traps while 249 km of line
   transects resulted in the detection of eight terrestrial mammals, 10
   arboreal mammals and five game bird species. A standardised methodology
   combining camera trapping and line transects is proposed for surveying
   mammals in remote areas. The combined methodology using camera trapping
   and line transects proved to be individually effective for target
   species while being highly complementary in detecting the presence of
   various terrestrial and arboreal mammals.}},
