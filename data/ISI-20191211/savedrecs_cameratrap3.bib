@article{ ISI:000372546000023,
Author = {McPherson, Shane C. and Brown, Mark and Downs, Colleen T.},
Title = {{Diet of the crowned eagle (Stephanoaetus coronatus) in an urban
   landscape: potential for human-wildlife conflict?}},
Journal = {{URBAN ECOSYSTEMS}},
