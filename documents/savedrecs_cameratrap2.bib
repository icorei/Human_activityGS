@article{ ISI:000404722800008,
Author = {Porfirio, Grasiela and Sarmento, Pedro and Foster, Vania and Fonseca,
   Carlos},
Title = {{Activity patterns of jaguars and pumas and their relationship to those
   of their potential prey in the Brazilian Pantanal}},
Journal = {{MAMMALIA}},
Year = {{2017}},
Volume = {{81}},
Number = {{4}},
Pages = {{401-404}},
Month = {{JUL}},
Abstract = {{Jaguar and puma are the largest cats in the American continent.
   Competition between both species is expected due to similarities in diet
   and habitat use. The objective of this study was to test whether
   temporal separation exists between these two species and to analyze
   whether their activity patterns coincide with those of some of their
   potential prey. We used data from camera trapping to estimate activity
   patterns and measure the overlap between activity distributions using
   kernel density. The activity of jaguars and pumas overlapped extensively
   and followed those of some of their potential prey, suggesting a
   potential for competition.}},
