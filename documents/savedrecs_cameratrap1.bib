@article{ ISI:000474469700001,
Author = {Marenk, Ziga and Krofel, Miha},
Title = {{USE OF ARTIFICIAL FEEDING SITES BY EUROPEAN MOUFLON (Ovis orientalis
   musimon) IN THE GORENJSKA REGION, SLOVENIA}},
Journal = {{ACTA SILVAE ET LIGNI}},
Year = {{2019}},
Number = {{118}},
Pages = {{1-11}},
Abstract = {{Mouflon (Ovis orientalis musimon) is currently the most abundant but
   poorly studied introduced ungulate species in Slovenia. Studies are rare
   also elsewhere across Europe, especially in respect to the use of
   artificial feeding sites, although supplemental feeding is common
   practice in many countries. We used camera-traps to determine group
   structure of mouflons using five winter feeding sites in the Gorenjska
   region (Julian Alps, NW Slovenia). We also studied their circadian
   activity and the use of mouflon feeding sites by other species. Average
   group size was 3.5 animals and feeding sites were most often used by
   ewes with their offspring. We recorded two peaks in circadian use of
   feeding sites at sunrise and sunset. We recorded six other species using
   the feeding sites, red deer (Cervus elaphus) being the most dominant
   among them. Use of camera-traps at the artificial feeding sites proved
   to be an efficient method to monitor mouflon population and their use of
   feeding sites.}},
