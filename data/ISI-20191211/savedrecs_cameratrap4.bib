@article{ ISI:000327380400007,
Author = {Carter, Neil H. and Gurung, Bhim and Vina, Andres and Campa, III, Henry
   and Karki, Jhamak B. and Liu, Jianguo},
Title = {{Assessing spatiotemporal changes in tiger habitat across different land
   management regimes}},
Journal = {{ECOSPHERE}},
