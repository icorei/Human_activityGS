@article{ ISI:000365704900017,
Author = {Kuijper, Dries P. J. and Bubnicki, Jakub W. and Churski, Marcin and
   Mols, Bjorn and van Hooft, Pim},
Title = {{Context dependence of risk effects: wolves and tree logs create patches
   of fear in an old-growth forest}},
Journal = {{BEHAVIORAL ECOLOGY}},
Year = {{2015}},
Volume = {{26}},
Number = {{6}},
Pages = {{1558-1568}},
Month = {{NOV-DEC}},
Abstract = {{Large mammalian carnivores create areas perceived as having high and low
   risk by their ungulate prey. Human activities can indirectly shape this
   landscape of fear by altering behavior and spatial distribution of
   carnivores. We studied how red deer perceive the landscape of fear in an
   old-growth forest system (Bialowieza Primeval Forest, Poland) both at
   large and fine spatial scale. Camera traps were placed at locations with
   and without tree logs (fine-scale risk factor) and at different
   distances from the core of a wolf territory and human settlements
   (large-scale risk factor). Red deer avoided coming close to large tree
   logs and increased their vigilance levels when they were present in
   close vicinity. The strength of these effects depended on the distance
   to the wolf core area; deer perceived tree logs as more risky when
   wolves were more often present. Hence, tree logs inside wolf core areas
   create fine-scale patches of fear with reduced deer browsing pressure,
   thereby enhancing chances for successful tree recruitment. Human
   presence shapes this landscape of fear as wolf core areas are located
   far from human settlements. This ``human shadow{''} on predator-prey
   interactions is therefore an important component that should be taken
   into account in human-dominated landscapes.}},
