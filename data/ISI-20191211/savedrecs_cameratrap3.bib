@article{ ISI:000397458000027,
Author = {Ramesh, Tharmalingam and Kalle, Riddhika and Rosenlund, Havard and
   Downs, Colleen T.},
Title = {{Low leopard populations in protected areas of Maputaland: a consequence
   of poaching, habitat condition, abundance of prey, and a top predator}},
Journal = {{ECOLOGY AND EVOLUTION}},
