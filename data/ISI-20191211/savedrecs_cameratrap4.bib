@article{ ISI:000313529200004,
Author = {Rustam and Yasuda, Masatoshi and Tsuyuki, Satoshi},
Title = {{Comparison of mammalian communities in a human-disturbed tropical
   landscape in East Kalimantan, Indonesia}},
