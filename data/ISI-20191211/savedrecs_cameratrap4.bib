@inproceedings{ ISI:000368235000026,
Author = {Nelson, Jenny L. and Scroggie, Michael P. and Belcher, Chris A.},
Editor = {{Ballard, G and Banks, P and Claridge, A and Sanderson, J and Swann, D}},
Title = {{Developing a camera trap survey protocol to detect a rare marsupial
   carnivore, the spotted-tailed quoll (Dasyurus maculatus)}},
Booktitle = {{CAMERA TRAPPING: WILDLIFE MANAGEMENT AND RESEARCH}},
Year = {{2014}},
Pages = {{271-279}},
Note = {{1st International Colloquium on Camera Trapping in Wildlife Management
   and Research, Sydney, AUSTRALIA, SEP 13-14, 2012}},
Abstract = {{The spotted-tailed quoll is a rare, forest-dependent marsupial carnivore
   that is often difficult to detect using traditional survey methods such
   as live trapping or hair tube sampling. Camera trapping may be a cost
   effective method of surveying the large areas occupied by quolls, but
   the survey effort required for an adequate probability of detection is
   unknown. In developing a reliable camera trap survey protocol we
   deployed one to four baited camera traps within 54 randomly selected, 1
   km(2) survey sites in the Alpine and Snowy River National Parks in
   eastern Victoria. We used occupancy modelling to investigate the
   relationship between four camera trap densities and the probability of
   detecting quolls within occupied sites. Spotted-tailed quolls were
   detected at eight of 52 operational camera sites. We found no consistent
   relationship between the probability of detecting quolls and camera trap
   density, with an estimated overall probability of detection > 0.8 after
   3 weeks of sampling, irrespective of camera density. Although we found
   no relationship between detection probability and camera trap density,
   this high probability of detection confirms that camera traps provide a
   feasible option for surveying and monitoring spotted-tailed quolls,
   particularly in areas of the species' range where their densities are
   low. In future surveys for spotted-tailed quolls, we recommend deploying
   two widely spaced camera traps (i.e. 500 m apart) on survey sites for at
   least 3 weeks. Surveys should aim to maximise the number of sites
   surveyed in an area to provide a precise assessment of the distribution
   and status of populations, rather than increasing the survey effort at
   each site.}},
