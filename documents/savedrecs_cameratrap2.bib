@article{ ISI:000436247100010,
Author = {Sedash, Gleb A. and Matiukhina, Dina S. and Sonin, Petr L. and
   Blidchenko, Ekaterina Yu. and Storozhuk, Victor B.},
Title = {{FIRST RECORDED CASE OF FEMALE BROWN BEAR (URSUS ARCTOS: URSIDAE) WITH
   FIVE SECOND YEAR CUBS}},
Journal = {{NATURE CONSERVATION RESEARCH}},
Year = {{2018}},
Volume = {{3}},
Number = {{2}},
Pages = {{100-103}},
Abstract = {{We report the first case of a super-large brown bear (Ursus arctos)
   litter surviving until the second year of life. In the Land of the
   Leopard National Park, using camera traps, we recorded a female with
   five cubs two times in March and in December 2017. Litters with more
   than four cubs are extremely rare for brown bears, and are known for
   newborn cubs only. We report the first documented evidence of a five-cub
   litter, which are very close to surviving through the first two years of
   their life while bear cub mortality is extremely high.}},
