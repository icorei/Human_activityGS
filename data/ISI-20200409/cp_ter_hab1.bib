@article{ ISI:000404380200002,
Author = {Elmore, R. Dwayne and Carroll, J. Matthew and Tanner, Evan P. and
   Hovick, Torre J. and Grisham, Blake A. and Fuhlendorf, Samuel D. and
   Windels, Steve K.},
Title = {{Implications of the Thermal Environment for Terrestrial Wildlife
   Management}},
