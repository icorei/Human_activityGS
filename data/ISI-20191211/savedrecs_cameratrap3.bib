@article{ ISI:000403577800013,
Author = {Frehner, Ethan H. and Buechley, Evan R. and Christensen, Tara and
   Sekercioglu, Cagan H.},
Title = {{SUBTERRANEAN CACHING OF DOMESTIC COW (BOS TAURUS) CARCASSES BY AMERICAN
   BADGERS (TAXIDEA TAXUS) IN THE GREAT BASIN DESERT, UTAH}},
Journal = {{WESTERN NORTH AMERICAN NATURALIST}},
Year = {{2017}},
Volume = {{77}},
Number = {{1}},
Pages = {{124-129}},
Month = {{MAR}},
Abstract = {{Camera traps documented 2 solitary American badgers (Taxidea taxus)
   independently caching juvenile domestic cow (Bos taurus) carcasses
   during late winter 2016 in the Great Basin Desert of Utah. One carcass
   was partially buried and the other was entirely buried. Both badgers
   constructed dens alongside their cache, where they slept, fed, and spent
   up to 11 days continuously underground. They abandoned the sites 41 and
   52 days after initial discovery. While badgers are known to scavenge and
   to cache small food items underground, this is the first evidence of an
   American badger caching an animal carcass larger than itself.}},
