@article{ ISI:000346857900018,
Author = {Charre-Medellin, Juan F. and Monterrubio-Rico, Tiberio C. and
   Guido-Lemus, Daniel},
Title = {{A new jaguar (Panthera onca) record for central western Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2014}},
Volume = {{85}},
Number = {{4}},
Pages = {{1295-1299}},
Month = {{DEC}},
Abstract = {{We recorded new distribution records of jaguar (Panthera onca) for the
   region known as the Lower Balsas Basin in Michoacan. These records
   constitute the clearest evidence of the species presence on the
