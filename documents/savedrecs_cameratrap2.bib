@article{ ISI:000428216300013,
Author = {Dobbins, Michael T. and Steinberg, Michael K. and Broadbent, Eben N. and
   Ryan, Sadie J.},
Title = {{Habitat use, activity patterns and human interactions with jaguars
   \&ITPanthera onca\&IT in southern Belize}},
Journal = {{ORYX}},
