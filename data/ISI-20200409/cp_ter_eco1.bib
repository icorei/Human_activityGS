@article{ ISI:000458575500016,
Author = {Giakoumi, Sylvaine and Hermoso, Virgilio and Carvalho, Silvia B. and
   Markantonatou, Vasiliki and Dagys, Mindaugas and Iwamura, Takuya and
   Probst, Wolfgang N. and Smith, Robert J. and Yates, Katherine L. and
